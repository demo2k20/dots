alias gh="cd ~ && ls -hN --color=auto --group-directories-first"
alias gc="cd ~/.config && ls -hN --color=auto --group-directories-first"
alias gs="cd ~/.local/bin && ls -hN --color=auto --group-directories-first"
alias gdt="cd ~/Repos/dots && ls -hN --color=auto --group-directories-first"
alias gu="cd ~/Documents/Uni && ls -hN --color=auto --group-directories-first"
alias gpe="cd ~/Phone && ls -hN --color=auto --group-directories-first"
alias grp="cd ~/Repos && ls -hN --color=auto --group-directories-first"
alias gdc="cd ~/Documents && ls -hN --color=auto --group-directories-first"
alias gdw="cd ~/Downloads && ls -hN --color=auto --group-directories-first"
alias gms="cd ~/Music && ls -hN --color=auto --group-directories-first"
alias gpc="cd ~/Pictures && ls -hN --color=auto --group-directories-first"
alias gvd="cd ~/Videos && ls -hN --color=auto --group-directories-first"
alias gwal="cd ~/Pictures/Wallpapers && ls -hN --color=auto --group-directories-first"
alias gmnt="cd /mnt && ls -hN --color=auto --group-directories-first"
alias gmed="cd /mnt/media && ls -hN --color=auto --group-directories-first"
alias gwin="cd /mnt/windows/Users/Roli && ls -hN --color=auto --group-directories-first"
